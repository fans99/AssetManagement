{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "layui@2.6.8",
      "destination": "lib/layui/"
    },
    {
      "library": "tailwindcss@2.2.7",
      "destination": "lib/tailwindcss/",
      "files": [
        "tailwind.min.css"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "moment.js@2.29.1",
      "destination": "lib/moment.js/",
      "files": [
        "moment.min.js",
        "locale/zh-cn.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-datetime-picker@2.4.4",
      "destination": "lib/bootstrap-datetime-picker/",
      "files": [
        "js/bootstrap-datetimepicker.min.js",
        "css/bootstrap-datetimepicker.css",
        "css/bootstrap-datetimepicker.min.css",
        "js/locales/bootstrap-datetimepicker.zh-CN.js"
      ]
    }
  ]
}